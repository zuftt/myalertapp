{
  "expo": {
    "name": "myalertApp",
    "slug": "myalertapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/android.png",
    "scheme": "myalertapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zuft3312.myalertapp",
      "googleServicesFile": "./GoogleService-Info.plist",
      "icon": "./assets/images/ios.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zuft_3312.myalertapp",
      "permissions": ["ACCESS_FINE_LOCATION"],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router", "expo-font", "expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "89665ab8-5cc5-4a99-ba65-05396124f0f8"
      }
    }
  }
}
